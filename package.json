{
  "name": "vue-firebase-startup-kit",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint --fix src/**/*.{vue,ts}",
    "lint:watch": "esw --watch --color --clear --fix --ext .vue src",
    "type-check:watch": "vue-tsc --noEmit --watch --allowjs"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "firebase": "^9.7.0",
    "firebaseui": "^6.0.1",
    "swiper": "^8.1.6",
    "vue": "^3.2.31",
    "vue-router": "^4.0.15"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@types/node": "^16.11.26",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^9",
    "babel-loader": "^8.2.4",
    "eslint": "^8.13.0",
    "eslint-plugin-vue": "^8.6.0",
    "eslint-watch": "^8.0.0",
    "sass": "^1.52.1",
    "typescript": "~4.6.3",
    "vite": "^2.9.1",
    "vue-loader": "^17.0.0",
    "vue-tsc": "^0.37.1"
  },
  "license": "MIT"
}